{
 "name": "reactjs-template",
 "version": "2.3.6",
 "private": true,
 "description": "A boilerplate for Next.js",
 "keywords": [
  "boilerplate",
  "next",
  "react",
  "template",
  "typescript"
 ],
 "homepage": "https://reactjs-template.vercel.app",
 "bugs": {
  "url": "https://github.com/hyoretsu/reactjs-template/issues",
  "email": "hyoretsu@gmail.com"
 },
 "repository": {
  "type": "git",
  "url": "https://github.com/hyoretsu/reactjs-template.git"
 },
 "license": "MIT",
 "author": {
  "name": "Aran Leite",
  "email": "hyoretsu@gmail.com",
  "url": "https://www.linkedin.com/in/aranleite"
 },
 "contributors": [
  {
   "name": "",
   "email": "",
   "url": ""
  }
 ],
 "scripts": {
  "build": "next build",
  "dev": "NODE_OPTIONS='--inspect' node server.js",
  "export": "next export",
  "test": "jest --coverage --watchAll false --passWithNoTests",
  "update": "pnpm up -L && git add package.json pnpm-lock.yaml",
  "prepare": "husky install",
  "transpileNext": "babel next.config.ts -o next.config.js",
  "transpileServer": "babel server.ts -o server.js",
  "transpileSitemap": "babel next-sitemap.ts -o next-sitemap.js --plugins add-module-exports",
  "prebuild": "npm run transpileNext && npm run transpileSitemap",
  "postbuild": "next-sitemap",
  "predev": "npm run transpileNext && npm run transpileServer",
  "preexport": "npm run transpileNext"
 },
 "lint-staged": {
  "src/**/*.(ts|tsx)": [
   "eslint --fix",
   "yarn test --bail --findRelatedTests"
  ]
 },
 "dependencies": {
  "@babel/runtime": "^7.16.3",
  "@linaria/core": "^3.0.0-beta.13",
  "@linaria/react": "^3.0.0-beta.13",
  "axios": "^0.24.0",
  "core-js": "^3.19.1",
  "next": "^12.0.3",
  "next-seo": "^4.28.1",
  "polished": "^4.1.3",
  "react": "^17.0.2",
  "react-dom": "^17.0.2",
  "react-icons": "^4.3.1",
  "scheduler": "^0.20.2",
  "use-context-selector": "^1.3.9",
  "yup": "^0.32.11"
 },
 "devDependencies": {
  "@babel/cli": "^7.16.0",
  "@babel/core": "^7.16.0",
  "@babel/plugin-transform-runtime": "^7.16.0",
  "@babel/preset-env": "^7.16.0",
  "@linaria/babel-preset": "^3.0.0-beta.13",
  "@linaria/shaker": "^3.0.0-beta.13",
  "@linaria/webpack-loader": "^3.0.0-beta.13",
  "@next/eslint-plugin-next": "^12.0.3",
  "@svgr/webpack": "^5.5.0",
  "@types/enzyme": "^3.10.10",
  "@types/jest": "^27.0.2",
  "@types/node": "^16.11.7",
  "@types/react": "^17.0.34",
  "@types/react-dom": "^17.0.11",
  "@types/webpack": "^5.28.0",
  "@types/yup": "^0.29.13",
  "@typescript-eslint/eslint-plugin": "^5.3.1",
  "@typescript-eslint/parser": "^5.3.1",
  "@wojtekmaj/enzyme-adapter-react-17": "^0.6.5",
  "axios-mock-adapter": "^1.20.0",
  "babel-jest": "^27.3.1",
  "babel-plugin-add-module-exports": "^1.0.4",
  "enzyme": "^3.11.0",
  "eslint": "^8.2.0",
  "eslint-config-airbnb": "^19.0.0",
  "eslint-config-next": "^12.0.3",
  "eslint-config-prettier": "^8.3.0",
  "eslint-import-resolver-typescript": "^2.5.0",
  "eslint-plugin-import": "^2.25.3",
  "eslint-plugin-import-helpers": "^1.1.0",
  "eslint-plugin-jsx-a11y": "^6.5.1",
  "eslint-plugin-prettier": "^4.0.0",
  "eslint-plugin-react": "^7.27.0",
  "eslint-plugin-react-hooks": "^4.3.0",
  "husky": "^7.0.4",
  "jest": "^27.3.1",
  "jest-enzyme": "^7.1.2",
  "lint-staged": "^11.2.6",
  "next-linaria": "^1.0.1-beta",
